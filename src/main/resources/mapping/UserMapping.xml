<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.testmanage.mapper.UserMapper">

    <sql id="table">my_user</sql>
    <insert id="addUser"></insert>
    <update id="updateUser"></update>
    <delete id="deleteUser"></delete>
    <select id="getUserByName" resultType="com.testmanage.entity.MyUser">
        select * from
        <include refid="table"></include>
        where
        username=#{username}
    </select>
    <select id="getAllUser" resultType="com.testmanage.entity.MyUser"></select>


</mapper>